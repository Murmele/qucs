   {
      "dest-filename": "perl-libs/install.sh",
      "commands": [
         "set -e",
         "function make_install {",
         "    mod_dir=$1",
         "    echo Install module: $mod_dir",
         "    cd $mod_dir",
         "    if [ -f 'Makefile.PL' ]; then",
         "        perl Makefile.PL && make install",
         "    elif [ -f 'Build.PL' ]; then",
         "        perl Build.PL && ./Build && ./Build install",
         "    else",
         "        echo 'No Makefile.PL or Build.PL found. Do not know how to install this module'",
         "        exit 1",
         "    fi",
         "}",
         "(make_install perl-libs/FileWhich)",
         "(make_install perl-libs/AlienBuild)",
         "(make_install perl-libs/FileChdir)",
         "(make_install perl-libs/CaptureTiny)",
         "(make_install perl-libs/pathTiny)",
